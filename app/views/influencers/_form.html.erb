
<%= simple_form_for(@influencer) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :ac_compte %>
    <%= f.input :ac_cdiposit %>
    <%= f.input :ac_curt %>
    <%= f.input :ac_llarg %>
    <%= f.input :ac_deute %>
    <%= f.input :ac_jubil %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
